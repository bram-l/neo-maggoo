{
  "name": "neo-maggoo",
  "version": "0.3.0",
  "description": "Futuristic Models & Collections for Neo4j",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "NODE_ENV=test JASMINE_CONFIG_PATH=jasmine.json node --harmony node_modules/jasmine/bin/jasmine.js",
    "cover": "NODE_ENV=test JASMINE_CONFIG_PATH=jasmine.json node --harmony ./node_modules/nyc/bin/nyc.js --reporter=lcov node_modules/jasmine/bin/jasmine.js",
    "docs": "esdoc"
  },
  "author": "Bram Loogman <bram@moderntimes.nu>",
  "license": "MIT",
  "dependencies": {
    "deepmerge": "^1.3.1",
    "fs-extra": "^0.30.0",
    "fs-promise": "^0.5.0",
    "maggoo": "^0.3.0",
    "neo4j-driver": "^1.1.0",
    "shortid": "^2.2.6"
  },
  "devDependencies": {
    "babel-plugin-istanbul": "^4.1.3",
    "babel-preset-es2017": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^3.5.0",
    "esdoc": "^0.5.2",
    "esdoc-node": "^1.0.0",
    "jasmine": "^2.6.0",
    "jasmine-fail-fast": "^2.0.0",
    "jasmine-promises": "^0.4.1",
    "jasmine-spec-reporter": "^4.1.0",
    "mocha": "^3.3.0",
    "nyc": "^10.3.2"
  },
  "nyc": {
    "include": [
      "lib/*.js"
    ],
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "babel": {
    "presets": [
      "es2017"
    ],
    "env": {
      "test": {
        "plugins": [
          "istanbul"
        ]
      }
    }
  }
}
